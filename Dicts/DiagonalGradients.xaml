	<!-- White to Color gradients-->
	<Style x:Key="D_txtWhiteBlueGradientBackgroundStyle" TargetType="{x:Type TextBlock}">
		<Setter Property="Background">
			<Setter.Value>
				<LinearGradientBrush StartPoint="1,0" EndPoint=".3,1">
					<GradientStop Color="AliceBlue" Offset="1" />
					<GradientStop Color="Blue" Offset="0" />
				</LinearGradientBrush>
			</Setter.Value>
		</Setter>
		<Setter Property="FontSize" Value="12" />
	</Style>

